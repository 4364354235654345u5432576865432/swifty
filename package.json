{
  "name": "Swifty",
  "version": "0.1.2",
  "description": "Modern, Lightweight, Fast and Free Password Manager",
  "repository": "https://github.com/alchaplinsky/swifty.git",
  "dependencies": {
    "classnames": "^2.2.6",
    "cryptr": "^4.0.2",
    "generate-password": "^1.4.2",
    "luxon": "^1.17.2",
    "nucleon": "^1.0.5",
    "parse-domain": "^2.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
