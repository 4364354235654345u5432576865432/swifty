  },
  "scripts": {
    "lint": "npx eslint --color src",
    "test": "yarn lint && bozon test"
  },
  "author": {
    "name": "Alex Chaplinsky",
    "email": "alchaplinsky@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
